
# Database Configuration
DB_HOST=localhost
DB_PORT=3306
DB_NAME=ott_database
DB_USER=your_username
DB_PASSWORD=your_password

# Crawler Configuration
# true: 백그라운드, false: 브라우저 창 표시
HEADLESS_MODE=true
# 브라우저 타임아웃(ms)
BROWSER_TIMEOUT=30000
# 각 액션 간 딜레이(ms)
ACTION_DELAY=300
# 스크롤 제한(0=무제한)
SCROLL_LIMIT=100
# 최대 동시 실행 탭 수
MAX_TABS=5

# Logging Configuration
LOG_LEVEL=INFO
# 로그 파일 경로(상대경로 권장)
LOG_FILE_PATH=logs/app.log
