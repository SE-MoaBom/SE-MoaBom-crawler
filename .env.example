# Database Configuration
DB_HOST=localhost
DB_PORT=5432
DB_NAME=ott_database
DB_USER=your_username
DB_PASSWORD=your_password

# Crawler Configuration
HEADLESS_MODE=true # false시 브라우저 창이 열림
BROWSER_TIMEOUT=30000 # 브라우저 타임아웃 (밀리초)
ACTION_DELAY=300 # 각 액션 간 딜레이 (밀리초)
SCROLL_LIMIT=100 # 스크롤 제한

# Logging Configuration
LOG_LEVEL=INFO
LOG_FILE_PATH=/var/log/crawler/app.log
